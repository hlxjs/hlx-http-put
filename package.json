{
  "name": "hlx-http-put",
  "version": "0.0.1",
  "description": "A writable stream to send HLS streams to an external HTTP/HTTPS server",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "xo",
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "test": "npm run lint && npm audit --audit-level high && ava --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hlxjs/hlx-http-put.git"
  },
  "keywords": [
    "video",
    "HLS",
    "webdav",
    "stream",
    "media"
  ],
  "author": "Kuu Miyazaki",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hlxjs/hlx-http-put/issues"
  },
  "homepage": "https://github.com/hlxjs/hlx-http-put#readme",
  "engines": {
    "node": ">=8.0.0"
  },
  "xo": {
    "esnext": true,
    "space": true,
    "rules": {
      "capitalized-comments": 0,
      "dot-notation": 0,
      "indent": 0,
      "new-cap": 0,
      "no-cond-assign": 0,
      "no-multi-assign": 0,
      "padding-line-between-statements": 0,
      "promise/prefer-await-to-then": 0,
      "import/no-dynamic-require": 0,
      "unicorn/catch-error-name": 0,
      "unicorn/filename-case": 0
    }
  },
  "dependencies": {
    "ava": "^2.2.0",
    "debug": "^4.1.1",
    "hls-parser": "^0.4.1",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.4",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.0",
    "sinon": "^7.3.2",
    "xo": "^0.24.0"
  }
}
